<?xml version='1.0' encoding='UTF-8'?>    
<!DOCTYPE hibernate-mapping PUBLIC    
          "-//Hibernate/Hibernate Mapping DTD 5.4//EN"    
          "http://hibernate.sourceforge.net/hibernate-mapping-5.4.dtd">    

<hibernate-mapping>
<class name="com.kgisl.hibernatemanytoone.Car" table="car">
<id name="carId">
<generator class="increment"></generator>
</id>

<property name="carBrand"></property>
<property name="carColor"></property>

</class>
</hibernate-mapping>

